{
  "name": "@agado/api",
  "version": "0.0.1",
  "main": "src/index.js",
  "scripts": {
    "build": "yarn lint && tsc --build tsconfig.json",
    "copy:conf": "cp -r package.json yarn.lock dist/",
    "dev": "npm-run-all --parallel watch-ts serve-debug",
    "lint": "yarn eslint . --ext .ts,.tsx",
    "prettyall": "prettier --single-quote --write 'src/**/*.*'",
    "prisma:generate": "yarn prisma generate",
    "prisma:migrate": "prisma migrate deploy && prisma db seed --preview-feature",
    "fixtures:cli": "ts-node ./fixtures/fixture.ts",
    "serve-debug": "SOURCE_MAP=true nodemon --inspect --config .nodemon.json dist/index.js",
    "start": "node dist/index.js",
    "test:watch": "yarn test --watchAll",
    "test": "CONFIG_PATH=./.env.test jest --forceExit --coverage --verbose  --detectOpenHandles",
    "watch-ts": "tsc -w --incremental"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kepennar/api-starter.git"
  },
  "keywords": [
    "Node.js",
    "starter",
    "api",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kepennar/api-starter/issues"
  },
  "homepage": "https://github.com/kepennar/api-starter#readme",
  "dependencies": {
    "@agado/model": "1.0.0",
    "@prisma/client": "2.20.1",
    "@koa/cors": "^3.1.0",
    "app-root-path": "^3.0.0",
    "convict-format-with-validator": "^6.0.1",
    "convict": "^6.0.1",
    "dotenv": "^8.2.0",
    "into-stream": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^5.0.1",
    "koa-helmet": "^6.1.0",
    "koa-morgan": "^1.0.1",
    "koa-multer": "^1.0.2",
    "koa-qs": "^3.0.0",
    "koa-router": "^10.0.0",
    "koa": "^2.13.1",
    "minio": "^7.0.18",
    "mjml": "^4.8.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.11",
    "sharp": "^0.27.0",
